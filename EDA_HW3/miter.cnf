c ================================================================
c  Miter CNF for combinational equivalence checking
c
c  Variable mapping
c    1  a   (PI)
c    2  b   (PI)
c    3  c   (PI)
c    4  d   = a XOR b
c    5  e   = b OR  c
c    6  f   = d AND e
c    7  g   = a AND ¬b
c    8  h   = b AND ¬a
c    9  i   = g AND c
c   10  j   = i XOR h
c   11  z   = f XOR j         ── miter output
c ================================================================

p cnf 11 28           c 11 vars, 28 clauses
c ------------------------------------------------
c  d  = (a XOR b)
c  XOR → 4 Tseitin clauses
c ------------------------------------------------
 1   2  -4  0         c  ( a ∨  b ∨ ¬d )
 1  -2   4  0         c  ( a ∨ ¬b ∨  d )
-1   2   4  0         c  (¬a ∨  b ∨  d )
-1  -2  -4  0         c  (¬a ∨ ¬b ∨ ¬d )

c ------------------------------------------------
c  e  = (b OR c)
c ------------------------------------------------
-2   5      0         c  (¬b ∨ e)
-3   5      0         c  (¬c ∨ e)
 2   3  -5  0         c  ( b ∨ c ∨ ¬e )

c ------------------------------------------------
c  f = d AND e
c ------------------------------------------------
-4  -5   6  0         c  (¬d ∨ ¬e ∨ f)
-6   4      0         c  (¬f ∨ d)
-6   5      0         c  (¬f ∨ e)

c ------------------------------------------------
c  g = a AND ¬b
c ------------------------------------------------
-1   2   7  0         c  (¬a ∨  b ∨ g)
-7   1      0         c  (¬g ∨ a)
-7  -2      0         c  (¬g ∨ ¬b)

c ------------------------------------------------
c  h = b AND ¬a
c ------------------------------------------------
-2   1   8  0         c  (¬b ∨  a ∨ h)
-8   2      0         c  (¬h ∨ b)
-8  -1      0         c  (¬h ∨ ¬a)

c ------------------------------------------------
c  i = g AND c
c ------------------------------------------------
-7  -3   9  0         c  (¬g ∨ ¬c ∨ i)
-9   7      0         c  (¬i ∨ g)
-9   3      0         c  (¬i ∨ c)

c ------------------------------------------------
c  j = i XOR h
c ------------------------------------------------
 9   8  -10 0         c  ( i ∨  h ∨ ¬j)
 9  -8   10 0         c  ( i ∨ ¬h ∨  j)
-9   8   10 0         c  (¬i ∨  h ∨  j)
-9  -8  -10 0         c  (¬i ∨ ¬h ∨ ¬j)

c ------------------------------------------------
c  z = f XOR j   (miter)
c ------------------------------------------------
 6  10  -11 0         c  ( f ∨  j ∨ ¬z)
 6 -10   11 0         c  ( f ∨ ¬j ∨  z)
-6  10   11 0         c  (¬f ∨  j ∨  z)
-6 -10  -11 0         c  (¬f ∨ ¬j ∨ ¬z)

c ------------------------------------------------
c  Force z = 1 so solver seeks a *counter-example*
c ------------------------------------------------
 11           0       c  (z)
